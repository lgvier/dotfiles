alias lar='ls -latrh'
alias https='http --default-scheme=https'

# alias tl='echo \\n`ls -rt | tail -1`:\\n && tail -f `ls -rt | tail -1`'
# tl () { file = `ls -rt`;  }
function tl() {
  echo *$1*(.om[1])
  tail -f *$1*(.om[1])
}

#alias vl='echo \\n`ls -rt | tail -1`:\\n && vim `ls -rt | tail -1`'
function vl() {
  echo *$1*(.om[1])
  vim *$1*(.om[1])
}

